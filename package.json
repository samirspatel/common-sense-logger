{
  "name": "common-sense-logger",
  "version": "1.0.0",
  "description": "An EKS-compatible TypeScript logger with configurable output formats (Datadog, Elasticsearch) and colored development output",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:elasticsearch": "jest test/elasticsearch.test.ts",
    "test:datadog": "jest test/datadog.test.ts",
    "example": "NODE_ENV=development ts-node test/example.ts",
    "example:dev": "NODE_ENV=development ts-node test/example.ts",
    "example:prod": "NODE_ENV=production ts-node test/example.ts",
    "test:dev": "NODE_ENV=development ts-node test/example.ts",
    "test:prod": "NODE_ENV=production ts-node test/example.ts"
  },
  "keywords": [
    "logger",
    "json",
    "typescript",
    "logging",
    "eks",
    "datadog",
    "elasticsearch",
    "kubernetes"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}

